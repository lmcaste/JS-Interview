{
  "presets": [
    "react",
    "es2015",
    "stage-1",
    "@babel/preset-flow",
    "@babel/preset-react", 
    "@babel/preset-env"
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", {
      "legacy": true
    }],
    ["@babel/plugin-proposal-class-properties", {
      "loose": true
    }],
    ["@babel/plugin-syntax-decorators"],
    ["emotion"]
  ]
}